{
  "name": "bobrilstrap",
  "version": "0.20.0",
  "description": "Bobril wrapper for bootstrap",
  "typescript": {
    "main": "index.ts"
  },
  "bobril": {
    "example": "example.ts",
    "title": "Bobrilstrap",
    "additionalResourcesDirectory": "example/assets",
    "head": "<meta name=\"viewport\" content=\"width=device-width,initial-scale=1\"><link rel=\"shortcut icon\" type=\"image/ico\" href=\"favicon.ico\" />",
    "compilerOptions": {
      "noImplicitAny": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "strictNullChecks": true
    }
  },
  "author": "Tomáš Růt",
  "license": "MIT",
  "dependencies": {
    "bobril": "^4.*",
    "bootstrap": "^3.3.6",
    "jquery": "^2.2.3"
  }
}